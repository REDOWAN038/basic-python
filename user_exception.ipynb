   "source": [
    "class Accident(Exception):\n",
    " def __init__(self,msg):\n",
    "  self.msg = msg\n",
    " def print_exception(self):\n",
    "  print(self.msg)\n",
    "\n",
    "try:\n",
    " raise Accident(\"crash two cars\")\n",
    "except Accident as e:\n",
    " print(e)\n",
    "finally:\n",
    " print(\"finally block\")"
   ]
  }
